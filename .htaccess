Options -Indexes
Options -MultiViews
RewriteEngine  on
# virtual pages first
RewriteCond %{REQUEST_FILENAME}   !-f
RewriteCond %{REQUEST_FILENAME}   !-d
RewriteRule    ^(.*)     inlay.php?virtual=true&path=$1 [NS,QSA,L]
# actual pages next
# ALL HTML pages are passed throguh Inlay
RewriteCond %{REQUEST_FILENAME}   \.html$
RewriteRule    ^(.*)     inlay.php?path=$1 [NS,QSA,L]